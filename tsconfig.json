{
    "compilerOptions": {
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "isolatedModules": true,
        "strictNullChecks": true,
        "downlevelIteration": true,
        "noUnusedLocals": true,
        "importHelpers": true,
        "outDir": "build/dist",
        "target": "es5",
        "lib": ["es6", "dom", "dom.iterable", "esnext"],
        "sourceMap": true,
        "rootDir": "src",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true
    },
    "exclude": [
        "node_modules",
        "build",
        "scripts",
        "acceptance-tests",
        "webpack",
        "jest",
        "src/setupTests.ts",
        "cypress.config.cjs",
        "cypress"
    ],
    "types": ["typePatches"]
}
